/**

 * @param {number[]} prices

 * @return {number}

 */

var maxProfit = function(prices) {

    var profit = 0;

    var min = Number.MAX_SAFE_INTEGER;

    prices.forEach(function(price) {

        profit = Math.max(profit, price - min);

        min = Math.min(min, price);

    });

    return profit;

};
